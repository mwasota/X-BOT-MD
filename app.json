{
  "name": "Xhriss_Md",
  "version": "3.0.0",
  "description": "Xhriss_Md Simple WhatsApp Bot",
  "logo": "https://telegra.ph/file/2912dd1d0665285ffc931.jpg",
  "keywords": ["whatsapp", "bot", "md", "multi-device"],
  "repository": "https://github.com/A-S-W-I-N-S-P-A-R-K-Y/X-BOT-MD",
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "Enter Your Session ID",
      "required": true
    },
    "HANDLER": {
      "description": "Enter Prefix For Commands",
      "required": false,
      "value": "."
    },
    "SUDO": {
      "description": "Enter Numbers With Country Code Without + Separated With , To Add More Numbers",
      "required": true,
      "value": "255688930205"
    },
    "STICKER_DATA": {
      "description": "Enter Your Sticker PackName And Author Name",
      "required": false,
      "value": "Xhriss_Md;Xhriss"
    },
    "AUDIO_DATA": {
      "description": "Enter Your Owner Name",
      "required": false,
      "value": "Xhriss_Md;Xhriss;https://i.imgur.com/fVCRCYG.jpeg"
    },
    "BOT_INFO": {
      "description": "Enter Your Owner Number",
      "required": false,
      "value": "Xhriss_Md;Xhriss;https://i.imgur.com/r3GZeiX.jpeg"
    },
    "ALWAYS_ONLINE": {
      "description": "Enter Your Bot Name",
      "required": false,
      "value": "true"
    },
    "WORK_TYPE": {
      "description": "Enter Your Mode Type public For Public Otherwise Type private",
      "required": false,
      "value": "private"
    },
    "AUTO_STATUS_VIEW": {
      "description": "If You Want To View Status Automatically, Type true. Otherwise, Type false",
      "required": false,
      "value": "true"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    }
  ]
}
